{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:11304",
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "EtwEvents.AgentManager Dev": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:50300"
    },
    "EtwEvents.AgentManager Prod": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "https://localhost:50300"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "environmentVariables": {
        "Kestrel:Endpoints:Https:Certificate:Path": "/app/certs/localhost.p12",
        "Kestrel:Endpoints:Https:Certificate:Password": "dummy"
      },
      "launchUrl": "https://localhost:50300",
      "DockerfileRunArguments": "-v {ProjectDir}\\certificates:/app/certs:ro",
      "httpPort": 50301,
      "useSSL": true,
      "sslPort": 50300
    }
  }
}