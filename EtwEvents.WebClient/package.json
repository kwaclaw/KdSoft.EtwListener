{
    "name": "etw-events-webclient",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "postcss-fontawesome": "postcss node_modules/@fortawesome/fontawesome-free/css/all.css -o wwwroot/css/fontawesome/css/all.css",
        "postcss-src": "postcss src/css/**/*.pcss --dir wwwroot/css --ext css",
        "postinstall": "snowpack install --dest wwwroot/lib",
        "lint": "eslint",
        "po2json": "po2json-gettextjs wwwroot/de_AT.po wwwroot/de_AT.json -p",
        "build": "npm run postcss-fontawesome && npm run postcss-src && npm run po2json"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.1",
        "@kdsoft/lit-mvvm": "^1.0.0",
        "@nx-js/observer-util": "^4.2.2",
        "@nx-js/queue-util": "^1.1.1",
        "autoprefixer": "^10.1.0",
        "dialog-polyfill": "^0.5.4",
        "gettext.js": "^1.0.0",
        "lit-html": "^1.3.0",
        "tailwindcss": "^2.0.2"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.2.1",
        "eslint": "^7.17.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "postcss": "^8.2.2",
        "postcss-cli": "^8.3.1",
        "postcss-copy": "^7.1.0",
        "postcss-import": "^14.0.0",
        "postcss-modules": "^4.0.0",
        "snowpack": "^2.18.5"
    },
    "snowpack": {
        "exclude": [
            "**/node_modules/**/*"
        ],
        "install": [
            "@fortawesome/fontawesome-free",
            "@kdsoft/lit-mvvm",
            "@nx-js/observer-util/dist/es.es6.js",
            "@nx-js/queue-util/dist/es.es6.js",
            "lit-html",
            "lit-html/directives/repeat.js",
            "lit-html/directives/class-map.js",
            "gettext.js",
            "dialog-polyfill",
            "dialog-polyfill/dialog-polyfill.css"
        ]
    },
    "browserslist": [
        "last 2 chrome versions",
        "last 2 firefox versions",
        "last 2 safari versions"
    ],
    "-vs-binding": {
        "ProjectOpened": [
            "snowpack"
        ]
    }
}
