{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Error"
    },
    "RollingFile": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "Directory": "Logs",
      // DateTime will be passed to format function
      "FileNameFormat": "etw-pushagent-{0:yyyy-MM-dd}",
      "FileExtension": ".log",
      "UseLocalTime": true,
      "FileSizeLimitKB": 4096,
      "MaxFileCount": 11,
      "NewFileOnStartup": true,
      "BatchSize": 99,
      "MaxWriteDelayMSecs": 380
    }
  },
  "Control": {
    "Uri": "https://agent-manager.kd-soft.net:5300",
    "ClientCertificate": {
      "Location": "LocalMachine",
      "SubjectCN": "test-site-1"
    }
    // use defaults here:
    // "InitialRetryDelay": "00:00:01",
    // "MaxRetryDelay": "00:00:30",
    // "BackoffResetThreshold":  "00:01:00"
  },
  "EventQueue": {
    "LogPath": "EventQueue/evt.log"
  }
}
