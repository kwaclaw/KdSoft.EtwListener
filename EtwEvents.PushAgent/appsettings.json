{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "RollingFile": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "Directory": "Logs",
      "FileNameFormat": "etw-pushagent-{0:yyyy-MM-dd}",
      "FileExtension": ".log",
      "UseLocalTime": true,
      "FileSizeLimitKB": 4096,
      "MaxFileCount": 11,
      "NewFileOnStartup": true,
      "BatchSize": 99,
      "MaxWriteDelayMSecs": 380
    }
  },
  "Control": {
    "Uri": "https://agent-manager.kd-soft.net:5300",
    // use defaults here:
    // "InitialRetryDelay": "00:00:01",
    // "MaxRetryDelay": "00:00:30",
    // "BackoffResetThreshold":  "00:01:00"
  },
  "EventQueue": {
    "LogPath": "EventQueue/evt.log"
  },
  "EventSession": {
    "Name": "SmartClinicServices",
    "BatchSize": 127,
    "MaxWriteDelayMSecs": 800,
    "Providers": [
      {
        "name": "SmartClinic-Services-Mobility",
        "level": 4,
        "matchKeyWords": 0
      },
      {
        "name": "Microsoft-Extensions-Logging",
        "level": 4,
        "matchKeyWords": 0
      },
      {
        "name": "Microsoft-Windows-Application Server-Applications",
        "level": 4,
        "matchKeyWords": 2305843009213825000
      }
    ],
    "Filters": [
      "return true;"
    ],
    "ActiveFilterIndex": 0
  },
  "EventSink": {
    "Name": "Default",
    "SinkType": "ElasticSink",
    "Definition": {
      "Options": {
        "Nodes": [
          "http://10.106.122.211:9200"
        ],
        "Index": "smartclinic-logs-test"
      },
      "Credentials": {
        "User": "user",
        "Password": "pwd"
      }
    }
  }
}
