{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning"
    },
    "RollingFile": {
      "LogLevel": {
        "KdSoft.EtwEvents": "Information"
      },
      "Directory": "Logs",
      // DateTime will be passed to format function
      "FileNameFormat": "etw-pushagent-{0:yyyy-MM-dd}",
      "FileExtension": ".log",
      "UseLocalTime": true,
      "FileSizeLimitKB": 4096,
      "MaxFileCount": 11,
      "NewFileOnStartup": true,
      "BatchSize": 99,
      "MaxWriteDelayMSecs": 380
    }
  },
  "Control": {
    "Uri": "https://localhost:50300",
    "ClientCertificate": {
      "Location": "LocalMachine",
      //"SubjectCN": "test-site-1",
      "SubjectRole": "etw-pushagent"
    }
    // use defaults here:
    // "InitialRetryDelay": "00:00:01",
    // "MaxRetryDelay": "00:00:30",
    // "BackoffResetThreshold":  "00:01:00"
  },
  "EventQueue": {
    "BaseDirectory": "EventQueue",
    "FileName": "evt.log"
  }
}
