oid_section = oids

[oids]
dom=1.3.6.1.4.1.311.20.2
MsCaV=1.3.6.1.4.1.311.21.1
msEFSFR=1.3.6.1.4.1.311.10.3.4.1
iKEIntermediate=1.3.6.1.5.5.8.2.2
